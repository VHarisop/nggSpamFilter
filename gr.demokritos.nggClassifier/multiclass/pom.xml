<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
  <modelVersion>4.0.0</modelVersion>
  <groupId>gr.demokritos.nggClassifier</groupId>
  <artifactId>gr.demokritos.nggClassifier</artifactId>
  <packaging>jar</packaging>
  <version>1.0</version>
  <name>gr.demokritos.nggClassifier</name>
  <url>http://maven.apache.org</url>


  <parent>
	<groupId>gr.demokritos.nggClassifier</groupId>
	<artifactId>nggParent</artifactId>
	<version>1.0</version>
  </parent>

  <build>
    <plugins>
      <plugin>
        <groupId>org.apache.maven.plugins</groupId>
        <artifactId>maven-compiler-plugin</artifactId>
        <configuration>
          <target>1.5</target>
          <source>1.5</source>
        </configuration>
      </plugin>
    </plugins>
  </build>

  <dependencies>
    <dependency>
      <groupId>junit</groupId>
      <artifactId>junit</artifactId>
      <version>3.8.1</version>
      <scope>test</scope>
    </dependency>
	<!-- JInsect.jar dependency -->
	<dependency>
		<groupId>gr.demokritos.nggClassifier</groupId>
		<artifactId>jinsect</artifactId>
		<version>1.0</version>
	</dependency>
	
	<!-- OpenJGraph.jar dependency -->
	<dependency>
		<groupId>gr.demokritos.nggClassifier</groupId>
		<artifactId>openjgraph</artifactId>
		<version>1.0</version>
	</dependency>

	<dependency>
		<groupId>gr.demokritos.dataTools</groupId>
		<artifactId>dataTools</artifactId>
		<version>1.0</version>
	</dependency>

  </dependencies>
</project>
